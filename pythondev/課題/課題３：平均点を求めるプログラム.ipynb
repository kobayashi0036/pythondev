   "source": [
    "score=int(input(\"点数を入力してください：\"))\n",
    "totalScore=0\n",
    "average=0\n",
    "student=0\n",
    "\n",
    "while score!=-1:\n",
    "    student+=1\n",
    "    totalScore=totalScore+score\n",
    "    average=totalScore/student\n",
    "    score=int(input(\"点数を入力してください：\"))\n",
    "\n",
    "print(f\"{student}人のテストの平均点は{average}点です\")"
   ]
  }
